<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HTMLPanel><h2 ui:field="authorName">Author Name</h2></g:HTMLPanel>			
			<g:HorizontalPanel>
				<g:HTMLPanel ui:field="similarAuthors"/>
				<g:HTMLPanel ui:field="coAuthors"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:HTMLPanel ui:field="citedAuthors"/>
				<g:HTMLPanel ui:field="citedByAuthors"/>
			</g:HorizontalPanel>
			<g:HTMLPanel ui:field="paperByAuthor"/>		
		</g:VerticalPanel>
	</g:HTMLPanel>	
</ui:UiBinder>
