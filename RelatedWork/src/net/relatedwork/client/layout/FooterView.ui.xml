<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	<ui:with type="net.relatedwork.client.place.NameTokens"
		field="nameTokens"></ui:with>

	<g:HTMLPanel styleName="rwFooter">
			<g:VerticalPanel styleName="rwFooterBlock">
				<g:Hyperlink targetHistoryToken="{nameTokens.getImprint}">Imprint</g:Hyperlink>
				<g:Hyperlink targetHistoryToken="{nameTokens.getAbout}">About</g:Hyperlink>
				<g:Anchor href="http://blog.related-work.net/blog/">Blog</g:Anchor>
			</g:VerticalPanel>
			<g:VerticalPanel styleName="rwFooterBlock">
				<g:Hyperlink targetHistoryToken="{nameTokens.getData}">Data</g:Hyperlink>
				<g:Hyperlink targetHistoryToken="{nameTokens.getLicense}">Licence</g:Hyperlink>
				<g:Anchor href="http://github.com/renepickhardt/related-work.net">Source Code</g:Anchor>
			</g:VerticalPanel>
			<g:VerticalPanel styleName="rwFooterBlock">
				<g:Hyperlink>Developer</g:Hyperlink>
				<g:Anchor href="https://github.com/renepickhardt/related-work.net/Open-ends">Open Ends</g:Anchor>
				<g:Anchor href="https://github.com/renepickhardt/related-work.net/Roadmap">Road Map</g:Anchor>
				<g:Anchor href="https://github.com/renepickhardt/related-work.net/issues">Bug Tracker</g:Anchor>
			</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
